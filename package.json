{
  "name": "natureza",
  "version": "1.0.0",
  "description": "pagina natureza template website",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass scss/main.scss css/style.css -w",
    "compile": "node-sass scss/main.scss css/style.comp.css",
    "concat": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "compress": "node-sass css/style.concat.css css/style.css --output-style compressed",
    "build": "npm-run-all compile concat compress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mafideju/ThemeNatureza.git"
  },
  "author": "Marcio Mafideju Rodrigues",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mafideju/ThemeNatureza/issues"
  },
  "homepage": "https://github.com/mafideju/ThemeNatureza#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {}
}
